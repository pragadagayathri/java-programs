//declaring variables in method and accessing them
class First
{
	int x=10,y=20;
	public void display(){               
		System.out.println("Good Morning");
	}
	public static void main(String args[])
	{
		First obj=new First();
		obj.display();
		System.out.println("Welcome");
		System.out.println(obj.x+" "+obj.y);
	}
}

